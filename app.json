{
  "expo": {
    "name": "noFap2",
    "slug": "noFap2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/nofap-icon.png",
    "scheme": "nofap2",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#8B4513",
        "foregroundImage": "./assets/images/nofap-logo-foreground.png",
        "backgroundImage": "./assets/images/nofap-logo-background.png",
        "monochromeImage": "./assets/images/nofap-logo-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/nofap-favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/nofap-notification-icon.png",
          "color": "#FFD700",
          "defaultChannel": "default"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/nofap-splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#8B4513",
          "dark": {
            "backgroundColor": "#4A2C1A"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
