{
  "expo": {
    "name": "noFap",
    "slug": "noFap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/nofap-icon.png",
    "scheme": "nofap",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.nofap.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#8B4513",
        "foregroundImage": "./assets/images/nofap-logo-foreground.png",
        "backgroundImage": "./assets/images/nofap-logo-background.png",
        "monochromeImage": "./assets/images/nofap-logo-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.NOTIFICATIONS",
        "android.permission.VIBRATE"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/nofap-favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/nofap-notification-icon.png",
          "color": "#FFD700",
          "defaultChannel": "default"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/nofap-splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#8B4513",
          "dark": {
            "backgroundColor": "#4A2C1A"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b535195e-3995-4afa-952c-6180c7c4c13a"
      }
    },
    "owner": "pk_haha"
  }
}
